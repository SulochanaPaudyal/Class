#subclass of animal 
class cat(animal): #cat parent is Animal, evth animal has now the cat has 
    def speak(self): #new functionality #__init__is not missing, uses Animal version 
        print("meow")
    def __str__(self): #overriden the str method of the animal class 
        return "cat:"+ str(self.name)+ str(self.age)
        
class Person(animal):
